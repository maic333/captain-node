{
  "env": {
    "browser": true,
    "jasmine": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "_": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "arrow-parens": 2,
    "curly": 2,
    "eqeqeq": 2,
    "no-cond-assign": 2,
    "no-console": [
      2,
      {
        "allow": [
          "debug",
          "info",
          "error"
        ]
      }
    ],
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 0,
    "no-extra-parens": 0,
    "no-extra-semi": 2,
    "no-irregular-whitespace": 2,
    "no-multi-spaces": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-return-assign": 2,
    "no-sparse-arrays": 2,
    "no-undef": 0,
    "no-unused-expressions": ["error", { "allowShortCircuit": true }],
    "no-unreachable": 2,
    "no-useless-escape": 0,
    "no-var": 2,
    "object-shorthand": 0,
    "strict": 0,
    "use-isnan": 2,
    "valid-typeof": 2,
    "array-bracket-spacing": [
      2,
      "never"
    ],
    "block-spacing": 2,
    "brace-style": 2,
    "camelcase": 2,
    "comma-dangle": [
      2,
      "never"
    ],
    "comma-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "comma-style": [
      2,
      "last"
    ],
    "computed-property-spacing": [
      2,
      "never"
    ],
    "func-call-spacing": [
      2,
      "never"
    ],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "jsx-quotes": [
      2,
      "prefer-single"
    ],
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "keyword-spacing": [
      2,
      {
        "before": true,
        "after": true
      }
    ],
    "new-cap": 0,
    "new-parens": 2,
    "newline-before-return": 0,
    "no-bitwise": 2,
    "no-lonely-if": 0,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": 2,
    "no-trailing-spaces": 2,
    "no-whitespace-before-property": 2,
    "object-curly-spacing": [
      2,
      "never"
    ],
    "object-property-newline": 0,
    "operator-linebreak": [
      2,
      "after"
    ],
    "quotes": [
      2,
      "single"
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "spaced-comment": [
      0,
      "always"
    ],
    "no-extend-native": 2,
    "no-use-before-define": [
      2,
      {
        "functions": false
      }
    ],
    "no-unused-vars": 2,
    "max-params": [
      2,
      15
    ],
    "max-depth": [
      2,
      5
    ],
    "max-statements": [
      2,
      100
    ],
    "complexity": 0,
    "max-len": [
      2,
      9999
    ],
    "semi": 2,
    "no-loop-func": 0,
    "no-shadow": 2,
    "space-before-blocks": 2
  }
}